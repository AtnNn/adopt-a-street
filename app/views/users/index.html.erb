<p id="notice"><%= notice %></p>

<h1>Listing Users</h1>

<table>
  <thead>
    <tr>
      <th>Primary first name</th>
      <th>Primary last name</th>
      <th>Secondary first name</th>
      <th>Secondary last name</th>
      <th>Primary phone</th>
      <th>Secondary phone</th>
      <th>Primary email</th>
      <th>Secondary email</th>
      <th>Address</th>
      <th>Organization</th>
      <th>Salutation</th>
      <th>Phone type</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.primary_first_name %></td>
        <td><%= user.primary_last_name %></td>
        <td><%= user.secondary_first_name %></td>
        <td><%= user.secondary_last_name %></td>
        <td><%= user.primary_phone %></td>
        <td><%= user.secondary_phone %></td>
        <td><%= user.primary_email %></td>
        <td><%= user.secondary_email %></td>
        <td><%= user.address %></td>
        <td><%= user.originization %></td>
        <td><%= user.salutation %></td>
        <td><%= user.phone_type %></td>
        <td><%= link_to 'Show', user %></td>
        <td><%= link_to 'Edit', edit_user_path(user) %></td>
        <td><%= link_to 'Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User', new_user_path %>
